{
	"name": "discordbot",
	"version": "0.5.0",
	"description": "A bot for Discord written in Node.js without any third-party libraries.",
	"main": "emit/main.js",
	"type": "module",
	"scripts": {
		"start": "npx tsc && node .",
		"clean": "rm -r emit",
		"build": "rm -r emit && npx tsc --sourceMap false && docker build --pull --tag bot:latest .",
		"docker": "docker run --name bot --interactive --tty --env-file .env --rm bot:latest"
	},
	"author": {
		"name": "viral32111",
		"email": "contact@viral32111.com",
		"url": "https://viral32111.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/viral32111/discordbot"
	},
	"keywords": [
		"discord",
		"bot",
		"automation"
	],
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"@types/node": "^18.0.0",
		"dotenv": "^16.0.1",
		"typescript": "^4.7.4"
	}
}
